nums = list(map(int, input("Enter positive numbers separated by spaces: ").split()))
n = len(nums)
for i in range(n-1):
    if nums[i] == nums[i+1]:
        nums[i] *= 2
        nums[i+1] = 0
    else:
        for i in range(n):
            if i == 0:
                nums.remove(i)
                nums.append(i)
print(nums)
